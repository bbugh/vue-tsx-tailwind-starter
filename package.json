{
  "name": "vue-tsx",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint:code": "eslint . --ext .ts,.js,.tsx",
    "lint:style": "stylelint src/**/*.css",
    "lint": "concurrently -c bgBlue.bold,bgMagenta.bold -n code,style yarn:lint:code yarn:lint:style",
    "prepare": "husky install",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.1.1",
    "concurrently": "^6.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.11.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "postcss": "^8.3.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vite-plugin-ts-checker": "^0.1.5",
    "vite-plugin-windicss": "^1.1.0",
    "windicss": "^3.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn lint:code --fix",
    "*.css": "yarn lint:style --fix"
  }
}
