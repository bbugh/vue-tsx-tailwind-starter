{
  "name": "vue-tsx",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint:code": "eslint . --ext .ts,.js,.tsx",
    "lint:style": "stylelint src/**/*.css",
    "lint": "concurrently -c bgBlue.bold,bgMagenta.bold -n code,style yarn:lint:code yarn:lint:style",
    "prepare": "husky install",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "3.1.5"
  },
  "devDependencies": {
    "@types/node": "15.12.5",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "@vitejs/plugin-vue": "1.2.5",
    "@vitejs/plugin-vue-jsx": "1.1.7",
    "@vue/compiler-sfc": "3.1.5",
    "concurrently": "6.2.0",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.12.1",
    "husky": "6.0.0",
    "lint-staged": "11.0.1",
    "postcss": "8.3.6",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.3.2",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "typescript": "4.3.5",
    "vite": "2.3.8",
    "vite-plugin-ts-checker": "0.1.5",
    "vite-plugin-windicss": "1.2.5",
    "windicss": "3.1.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn lint:code --fix",
    "*.css": "yarn lint:style --fix"
  }
}
